[tool.poetry]
name = "usap-xapp"
version = "0.0.1"
description = "USAP xApp source code"
authors = ["Murilo Silva <murilosilva@itec.ufpa.br>"]
readme = "README.md"
packages = [{include = "app"}]

[tool.poetry.dependencies]
python = "^3.12"
hiredis = "^3.1.0"
ricxappframe = "^3.2.2"
six = "^1.17.0"
asn1tools = "^0.167.0"
redis-py-cluster = "^2.1.3"

[build-system]
requires = ["poetry-core"]
build-backend = "poetry.core.masonry.api"

[tool.poetry.scripts]
xapp = "app.main:run"