{
    "name": "usap-xapp",
    "version": "0.0.1",
    "appVersion": "0.0.1",
    "vendor": "Murilo Silva <murilosilva@itec.ufpa.br>",
    "containers": [
        {
            "name": "usap-xapp",
            "image": {
                "registry": "docker.io",
                "name": "muriloavlis/usap-xapp",
                "tag": "0.0.1"
            }
        }
    ],
    "livenessProbe": {
        "httpGet": {
            "path": "ric/v1/health/alive",
            "port": 8080
        },
        "initialDelaySeconds": 5,
        "periodSeconds": 15
    },
    "readinessProbe": {
        "httpGet": {
            "path": "ric/v1/health/ready",
            "port": 8080
        },
        "initialDelaySeconds": 5,
        "periodSeconds": 15
    },
    "messaging": {
        "ports": [
            {
                "name": "http",
                "container": "usap-xapp",
                "port": 8080,
                "description": "http service"
            },
            {
                "name": "rmr-route",
                "container": "usap-xapp",
                "port": 4561,
                "description": "rmr route port for usap"
            },
            {
                "name": "rmr-data",
                "container": "usap-xapp",
                "port": 4560,
                "protocol": "tcp",
                "maxSize": 65536,
                "numWorkers": 1,
                "rxMessages": [
                    "RIC_SUB_RESP",
                    "RIC_SUB_FAILURE",
                    "RIC_SUB_DEL_RESP",
                    "RIC_INDICATION"
                ],
                "txMessages": [
                    "RIC_SUB_REQ",
                    "RIC_SUB_DEL_REQ"
                ],
                "policies": [],
                "description": "rmr data port for usap-xapp"
            }
        ]
    },
    "rmr": {
        "protPort": "tcp:4560",
        "maxSize": 2072,
        "numWorkers": 1,
        "rxMessages": [
            "RIC_SUB_RESP",
            "RIC_INDICATION",
            "RIC_HEALTH_CHECK_REQ"
        ],
        "txMessages": [
            "RIC_SUB_REQ",
            "RIC_HEALTH_CHECK_RESP"
        ],
        "policies": []
    },
    "controls": {
        "active": true,
        "measurement_interval": 1000,
        "interfaceId": {
            "globalGNBId": {
                "plmnId": "00101"
            }
        },
        "logger": {
            "level": 4
        },
        "subscription": {
            "subscriptionActive": true,
            "e2sm": [
                {
                    "name": "KPM",
                    "ran_function_id": 2,
                    "reporting_period": 1000,
                    "granularity_period": 1000,
                    "report_style_type": 4,
                    "ue_list": [
                        {
                            "id": 0
                        }
                    ]
                },
                {
                    "name": "RC"
                }
            ]
        },
        "localAddress": {
            "host": ":8080"
        }
    },
    "db": {
        "waitForSdl": false,
        "maxIdle": 50,
        "maxActive": 100
    },
    "coredb": {
        "username": "test",
        "password": "test",
        "hostname": "mysql.oai5gcn",
        "port": "3306",
        "dbname": "oai_db"
    },
    "metrics": {
        "url": "/ric/v1/metrics",
        "namespace": "ricxapp"
    },
    "faults": {},
    "measurements": [
        {
            "objectName": "DRB.UEThpDl",
            "objectInstance": "DRB.UEThpDl",
            "name": "ricxapp_usap_xapp_DRBUEThpDl",
            "type": "int",
            "description": "UE Downlink Throughput "
        },
        {
            "objectName": "DRB.UEThpUl",
            "objectInstance": "DRB.UEThpUl",
            "name": "ricxapp_usap_xapp_DRBUEThpUl",
            "type": "int",
            "description": "UE Uplink Throughput"
        }
    ]
}